{
  "name": "ug-data",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "11.11.1",
    "@types/react": "16.8.7",
    "@types/react-dom": "16.8.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8",
    "typescript": "3.3.3333"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --no-cache",
    "lint": "tslint -c tslint.json ./**/*.{ts,tsx}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.0",
    "jest-enzyme": "^7.0.2",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^3.6.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "collectCoverageFrom": [
      "**/index.{js,jsx,ts,tsx}",
      "!**/*stories.{js,jsx}",
      "!**/node_modules/**",
      "!**/stories/**",
      "!**/functions/**",
      "!**/build/**",
      "!**/e2etests/**",
      "!**/coverage/**"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
